{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "clipboard-bookmark",
  "title": "Clipboard Bookmark",
  "description": "Bookmark your clipboard",
  "icon": "icon.png",
  "author": "kikiding",
  "license": "MIT",
  "commands": [
    {
      "name": "clipboard-bookmark",
      "title": "Clipboard Bookmark",
      "description": "Bookmark your clipboard",
      "mode": "view"
    },
    {
      "name": "add-clipboard-bookmark",
      "title": "Paste and Bookmark",
      "description": "Paste and bookmark your current clipboard",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
